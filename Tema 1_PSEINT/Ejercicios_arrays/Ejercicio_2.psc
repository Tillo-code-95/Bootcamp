Algoritmo Ejercicio_1_arrays
	Definir my_array , indice ,tamano Como Entero;
	tamano = 5;
	Dimension my_array[tamano];
	
	
	
	Para indice = 0 hasta tamano - 1 Hacer
		Leer my_array[indice] ;
	FinPara	
	
	
	Para indice = tamano -1 hasta 0 Hacer
		
		Escribir " my_array  " ,  indice  ,  my_array[indice] " ";
	FinPara
     escribir " ";
	
FinAlgoritmo
